{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "consistent-return": 0,
    "quotes": 0,
    "import/newline-after-import": 0,
    "no-console": "off",
    "comma-dangle": "off"
  }
}